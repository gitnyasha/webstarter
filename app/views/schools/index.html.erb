<%= render 'layouts/header' %>
<h1>Schools</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.name %></td>
        <td><%= school.description %></td>
        <td><%= link_to 'Show', school %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

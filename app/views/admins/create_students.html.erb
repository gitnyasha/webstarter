<%= render 'dash' %>
<!-- Begin Page Content -->
<div class="container-fluid">
  <!-- Page Heading -->
  <%= link_to "Create Student", new_student_registration_path %>
  <h1>Students</h1>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Class</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @school_students.each do |student| %>
        <tr>
          <td><%= link_to student.email, student %></td>
          <td><%= student.grade_id %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
</div>
<!-- /.container-fluid -->
<%= render 'dashend' %>
